    "def SimpleSymbols(s):\n",
    "    s = '=' + s + '='\n",
    "    for i in s:\n",
    "        if i in 'abcdefghijklmnopqrstuvwxyz':\n",
    "            if not s[s.index(i) - 1] == '+' or not s[s.index(i) + 1] == '+':\n",
    "                return 'false'\n",
    "    return 'true'\n",
    "\n",
    "print(SimpleSymbols(input()))"
