    "    while(int(num) != 6174):\n",
    "        num_f = ''.join(sorted(str(num), reverse = False))\n",
    "        num_t = ''.join(sorted(str(num), reverse = True))\n",
    "        num = int(num_t) - int(num_f)\n",
    "        print (num_t, '-', num_f, '=', num)\n",
    "        if int(len(str(num))) < 4:\n",
    "            for item in range(4 - int(len(str(num)))):\n",
    "               num = str(num) + \"0\"\n",
    "        iter += 1\n",
    "    return iter\n",
